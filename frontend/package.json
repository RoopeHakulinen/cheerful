{
  "name": "cheerful",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:e2e": "ng serve --configuration=e2e",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --browsers ChromeHeadless --watch false",
    "lint": "ng lint",
    "deploy": "ng deploy --base-href=/cheerful/",
    "analyze-size": "ng build --sourceMap=true --output-hashing=none && source-map-explorer dist/frontend/main.js",
    "e2e": "playwright test -c ./e2e",
    "e2e:init": "playwright install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.6",
    "@angular/cdk": "14.2.4",
    "@angular/common": "14.2.6",
    "@angular/compiler": "14.2.6",
    "@angular/core": "14.2.6",
    "@angular/flex-layout": "14.0.0-beta.40",
    "@angular/forms": "14.2.6",
    "@angular/material": "14.2.4",
    "@angular/platform-browser": "14.2.6",
    "@angular/platform-browser-dynamic": "14.2.6",
    "@angular/router": "14.2.6",
    "@angular/service-worker": "14.2.6",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "ngx-drag-to-select": "5.0.1",
    "rx-query": "2.0.1",
    "rxjs": "7.5.7",
    "tslib": "2.4.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.6",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.2.6",
    "@angular/compiler-cli": "14.2.6",
    "@playwright/test": "1.27.1",
    "@types/jasmine": "4.3.0",
    "@types/node": "16.11.65",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "angular-cli-ghpages": "1.0.3",
    "eslint": "8.25.0",
    "jasmine-core": "4.4.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "source-map-explorer": "2.5.3",
    "typescript": "4.8.4"
  }
}

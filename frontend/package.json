{
  "name": "cheerful",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --browsers ChromeHeadless --watch false",
    "lint": "ng lint",
    "deploy": "ng deploy --no-silent --base-href=/cheerful/",
    "analyze-size": "ng build --sourceMap=true --output-hashing=none && source-map-explorer dist/frontend/main.js",
    "e2e": "playwright test -c ./e2e",
    "e2e:init": "playwright install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/flex-layout": "12.0.0-beta.35",
    "@angular/forms": "13.3.11",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "@angular/service-worker": "13.3.11",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "rxjs": "7.5.5",
    "tslib": "2.3.1",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.7",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.7",
    "@angular/compiler-cli": "13.3.11",
    "@playwright/test": "1.21.0",
    "@types/jasmine": "3.10.6",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "angular-cli-ghpages": "1.0.0",
    "eslint": "8.13.0",
    "jasmine-core": "3.99.1",
    "karma": "6.3.17",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "source-map-explorer": "2.5.2",
    "typescript": "4.6.3"
  }
}
